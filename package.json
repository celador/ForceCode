{
  "name": "forcecode",
  "version": "3.3.3",
  "publisher": "BrettWagner",
  "displayName": "ForceCode",
  "description": "Visual Studio Code extension for Salesforce (SFDC) development",
  "author": {
    "name": "CodeScience"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.23.0"
  },
  "icon": "images/new-logo.png",
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#FF4500",
    "theme": "dark"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/daishi4u/ForceCode.git"
  },
  "activationEvents": [
    "workspaceContains:force.json",
    "onCommand:ForceCode.showMenu"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "ForceCode.treeDataProvider",
          "name": "ForceCode: Running Tasks",
          "when": "ForceCodeActive"
        }
      ]
    },
    "commands": [
      {
        "command": "ForceCode.showMenu",
        "title": "ForceCode Menu",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.openOrg",
        "title": "Open Org In Browser",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.compile",
        "title": "Save/Deploy/Compile",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.refresh",
        "title": "Refresh from server",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.executeAnonymous",
        "title": "Execute Anonymous",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.open",
        "title": "Get Class, Page, or Trigger",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.staticResource",
        "title": "Bundle / Deploy Static Resource",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.openFileInOrg",
        "title": "Open this file in the Org",
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.toggleCoverage",
        "title": "Toggle Test Coverage",
        "icon": {
          "light": "./images/coverage.svg",
          "dark": "./images/coverage.svg"
        },
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.previewVF",
        "title": "Preview Visualforce page",
        "icon": {
          "light": "./images/logo.svg",
          "dark": "./images/logo.svg"
        },
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.previewApp",
        "title": "Preview Lightning App",
        "icon": {
          "light": "./images/logo.svg",
          "dark": "./images/logo.svg"
        },
        "category": "ForceCode"
      },
      {
        "command": "ForceCode.diff",
        "title": "Diff Local <> Remote",
        "category": "ForceCode"
      }
    ],
    "keybindings": [
      {
        "command": "ForceCode.showMenu",
        "key": "alt+cmd+c",
        "win": "ctrl+shift+c",
        "when": "editorFocus"
      },
      {
        "command": "ForceCode.compile",
        "key": "alt+cmd+s",
        "win": "ctrl+shift+s",
        "when": "editorFocus && ForceCodeActive"
      },
      {
        "command": "ForceCode.executeAnonymous",
        "key": "alt+cmd+e",
        "win": "ctrl+shift+e",
        "when": "editorFocus && resourceLangId == apex && ForceCodeActive && editorHasSelection"
      },
      {
        "command": "ForceCode.open",
        "key": "alt+cmd+o",
        "win": "ctrl:shift+o",
        "when": "ForceCodeActive"
      },
      {
        "command": "ForceCode.staticResource",
        "key": "alt+cmd+b",
        "win": "ctrl+shift+b",
        "when": "ForceCodeActive"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ForceCode configuration",
      "properties": {
        "force.filesExclude": {
          "type": "object",
          "default": {
            ".gitignore": true,
            ".DS_Store": true,
            ".org_metadata": true,
            "**/*.map": true,
            "node_modules/**": true,
            "bower_modules/**": true,
            "**.tmp": true,
            "**/*-meta.xml": true,
            ".log": true
          },
          "description": "An object describing the files to exclude from the Static Resource build. The key is a Nodejs \"Glob\" pattern, the value is true or false"
        }
      }
    },
    "languages": [
      {
        "id": "apex",
        "extensions": [
          ".apex",
          ".cls",
          ".trigger",
          ".tgr"
        ],
        "aliases": [
          "Apex"
        ]
      },
      {
        "id": "visualforce",
        "extensions": [
          ".component",
          ".page"
        ],
        "aliases": [
          "Visualforce"
        ]
      },
      {
        "id": "staticResource",
        "extensions": [
          ".resource"
        ]
      },
      {
        "id": "xml",
        "extensions": [
          ".xml",
          ".layout",
          ".resource",
          ".remoteSite",
          ".labels",
          ".dashboard",
          ".permissionset",
          ".workflow",
          ".email",
          ".profile",
          ".scf",
          ".queue",
          ".reportType",
          ".report",
          ".weblink",
          ".tab",
          ".letter",
          ".role",
          ".homePageComponent",
          ".homePageLayout",
          ".objectTranslation",
          ".flow",
          ".datacategorygroup",
          ".snapshot",
          ".site",
          ".sharingRules",
          ".settings",
          ".callCenter",
          ".community",
          ".authProvider",
          ".customApplicationComponent",
          ".quickAction",
          ".approvalProcess",
          ".auradoc",
          ".cmp",
          ".evt",
          ".tokens",
          ".design",
          ".object",
          ".app"
        ]
      },
      {
        "id": "apex",
        "aliases": [
          "Apex"
        ],
        "extensions": [
          ".apex"
        ]
      },
      {
        "id": "html",
        "aliases": [
          "HTML"
        ],
        "extensions": [
          ".html"
        ]
      },
      {
        "id": "apexCodeCoverage",
        "extensions": [
          ".acov"
        ]
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.toggleCoverage",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .page && ForceCodeActive",
          "command": "ForceCode.previewVF",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .app && ForceCodeActive",
          "command": "ForceCode.previewApp",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "ForceCodeActive",
          "command": "ForceCode.openOrg",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.compile",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == visualforce && ForceCodeActive",
          "command": "ForceCode.compile",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == xml && ForceCodeActive",
          "command": "ForceCode.compile",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.refresh",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == visualforce && ForceCodeActive",
          "command": "ForceCode.refresh",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == xml && ForceCodeActive",
          "command": "ForceCode.refresh",
          "group": "ForceCode"
        },
        {
          "when": "resourceExtname == .page && ForceCodeActive",
          "command": "ForceCode.previewVF",
          "group": "ForceCode"
        },
        {
          "when": "resourceExtname == .app && ForceCodeActive",
          "command": "ForceCode.previewApp",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.toggleCoverage",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == visualforce && ForceCodeActive",
          "command": "ForceCode.openFileInOrg",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.openFileInOrg",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && editorHasSelection && ForceCodeActive",
          "command": "ForceCode.executeAnonymous",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == visualforce && ForceCodeActive",
          "command": "ForceCode.diff",
          "group": "ForceCode"
        },
        {
          "when": "resourceLangId == apex && ForceCodeActive",
          "command": "ForceCode.diff",
          "group": "ForceCode"
        }
      ],
      "commandPalette": [
        {
          "command": "ForceCode.showMenu"
        },
        {
          "command": "ForceCode.compile",
          "when": "ForceCodeActive"
        },
        {
          "command": "ForceCode.refresh",
          "when": "ForceCodeActive"
        },
        {
          "command": "ForceCode.executeAnonymous",
          "when": "ForceCodeActive"
        },
        {
          "command": "ForceCode.open",
          "when": "ForceCodeActive"
        },
        {
          "command": "ForceCode.staticResource",
          "when": "ForceCodeActive"
        },
        {
          "command": "ForceCode.toggleCoverage",
          "when": "ForceCodeActive && resourceLangId == apex"
        },
        {
          "command": "ForceCode.diff",
          "when": "ForceCodeActive && (resourceLangId == apex || resourceLangId == visualforce)"
        },
        {
          "command": "ForceCode.openOrg",
          "when": "ForceCodeActive"
        }
      ]
    }
  },
  "scripts": {
    "bump": "npm version patch && git push && git push --tags && vsce package",
    "rebuild": "electron-rebuild -n 49 -v 1.3.8",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "clean": "rimraf ./out/ && rimraf ./node_modules/",
    "test": "node ./node_modules/vscode/bin/test",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "package": "vsce package"
  },
  "extensionDependencies": [
    "salesforce.salesforcedx-vscode-apex",
    "salesforce.salesforcedx-vscode-visualforce",
    "salesforce.salesforcedx-vscode-lightning"
  ],
  "dependencies": {
    "jsforce-metadata-tools": "^1.3.0",
    "crypto-browserify": "3.12.0",
    "fs-extra": "^6.0.1",
    "globule": "^1.2.1",
    "jsforce": "^1.8.4",
    "jszip": "^3.1.5",
    "klaw": "^2.1.1",
    "mime-types": "^2.1.18",
    "moment": "^2.22.2",
    "node-fetch": "^2.1.2",
    "os": "^0.1.1",
    "request-light": "^0.2.3",
    "salesforce-alm": "^43.5.0",
    "util.promisify": "^1.0.0",
    "vscode": "^1.1.18",
    "xml2js": "^0.4.19",
    "zip": "^1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/glob": "^5.0.35",
    "@types/mocha": "^5.2.4",
    "@types/node": "10.5.2",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "cross-env": "5.2.0",
    "decache": "^4.4.0",
    "electron-rebuild": "^1.8.1",
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "mocha-multi-reporters": "^1.1.7",
    "mock-spawn": "0.2.6",
    "nyc": "^12.0.2",
    "remap-istanbul": "^0.11.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.2",
    "source-map-support": "^0.5.6",
    "typescript": "^2.9.2",
    "vsce": "^1.43.0"
  }
}
